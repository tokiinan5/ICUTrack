<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.icutrack">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignIn" />
        <activity android:name=".SignUp" />
        <activity
            android:name=".WorkingActivity"
            android:label="@string/title_activity_working"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SavedData"
            android:label="@string/title_activity_saved_data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ApacheData" />
        <activity android:name=".Showdata" />
        <activity android:name=".IRPSData"></activity>
    </application>

</manifest>